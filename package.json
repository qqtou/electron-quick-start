{
    "name": "electron-quick-start",
    "version": "1.0.0",
    "description": "A minimal Electron application",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "packager": "electron-packager . seeleMist --all --out=../dist/ --overwrite=true --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --icon=seeleMist.ico",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "seeleMist",
        "mac": {
            "category": "your.app.category.type"
        }
    },
    "repository": "https://github.com/qqtou/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "GitHub",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "^3.0.4"
    },
    "dependencies": {
        "bluebird": "^3.5.2",
        "seele.js": "^1.5.1"
    }
}